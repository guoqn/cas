package com.ffcs.crmd.cas.core.ddd.api.dto;

import com.ffcs.crmd.cas.core.ddd.api.dto.impl.CrmdBaseDTO;

public class TreeNodeDTO extends CrmdBaseDTO {
	/**
	 * serialVersionUID.
	 */
	private static final long serialVersionUID = 1598179584838497003L;

	/**
	 * 节点标识.
	 */
	private String id;

	/**
	 * 父级节点.
	 */
	private String parent;

	/**
	 * 节点名称.
	 */
	private String text;

	/**
	 * 节点状态.
	 */
	private TreeNodeStateDTO state;

	/**
	 * 子节点.
	 */

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getParent() {
		return parent;
	}

	public void setParent(String parent) {
		this.parent = parent;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	public TreeNodeStateDTO getState() {
		return state;
	}

	public void setState(TreeNodeStateDTO state) {
		this.state = state;
	}
}
