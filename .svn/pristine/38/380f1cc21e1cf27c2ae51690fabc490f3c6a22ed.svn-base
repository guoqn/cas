package com.ffcs.crmd.cas.order.api.dto;

import com.ffcs.crmd.cas.core.ddd.api.dto.impl.CrmdBaseDTO;

/**
 * Created by YULIYAO on 2016/2/22.
 */
public class PreSaleOrderPoolAmountDTO extends CrmdBaseDTO {
    private static final long serialVersionUID = 1598179584838497003L;
    /**
     * 工单池总单量
     */
    private int totalAmount;

    /**
     * 未接收工单量
     */
    private int unAcceptAmount;

    /**
     * 可派单工单量
     */
    private int assignableAmount;

    /**
     * 员工个人接收工单数
     */
    private int staffReceivedAmount;

    /**
     * 团队工单池总数
     */
    private int orgAmount;

    private String staffName;

    public String getStaffName() {
        return staffName;
    }

    public void setStaffName(String staffName) {
        this.staffName = staffName;
    }

    public int getTotalAmount() {
        return totalAmount;
    }

    public void setTotalAmount(int totalAmount) {
        this.totalAmount = totalAmount;
    }

    public int getUnAcceptAmount() {
        return unAcceptAmount;
    }

    public void setUnAcceptAmount(int unAcceptAmount) {
        this.unAcceptAmount = unAcceptAmount;
    }

    public int getAssignableAmount() {
        return assignableAmount;
    }

    public void setAssignableAmount(int assignableAmount) {
        this.assignableAmount = assignableAmount;
    }

    public int getStaffReceivedAmount() {
        return staffReceivedAmount;
    }

    public void setStaffReceivedAmount(int staffReceivedAmount) {
        this.staffReceivedAmount = staffReceivedAmount;
    }

    public int getOrgAmount() {
        return orgAmount;
    }

    public void setOrgAmount(int orgAmount) {
        this.orgAmount = orgAmount;
    }
}
