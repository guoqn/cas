	<sql id="qryOrg" type="xml"> 
	<![CDATA[     
	   <dataSqlNode>
           <type>sqltext</type>
           <sqltext>
			select c.org_id,
			       c.common_region_id,
			       c.parent_org_id,
			       c.org_type,
			       c.org_content,
			       c.org_scale,
			       c.principal,
			       c.org_name,
			       c.org_name_en,
			       c.org_code,
			       c.area_code,
			       c.status_cd,
			       c.status_date,
			       c.create_date,
			       c.update_date,
			       c.org_duty,
			       c.area_id,
			       c.region_cd,
			       c.update_staff,
			       c.create_staff,
			       c.town_flag,
			       c.org_sub_type,
			       c.org_level,
			       c.org_sort,
			       c.party_id,
			       c.ctg_channel_nbr,
			       c.group_org_id,
			       c.group_org_code,
			       c.channel_type,
			       c.staff_id
			  from organization c where
			  c.status_cd = '1000'
 		</sqltext>     
            <dataSqlNode>
               <type>param</type>
               <sqltext> and c.org_name like ?</sqltext> 
               <paramTag>org_name</paramTag>
           </dataSqlNode>
           <dataSqlNode>
               <type>param</type>
               <sqltext> and c.org_id = ?</sqltext>
               <paramTag>org_id</paramTag>
           </dataSqlNode>
           <dataSqlNode>
               <type>param</type>
               <sqltext> and c.parent_org_id = ?</sqltext>
               <paramTag>parent_org_id</paramTag>
           </dataSqlNode>
           <dataSqlNode>
               <type>param</type>
               <sqltext> and c.org_id in </sqltext>
               <paramTag>orgIds</paramTag>
           </dataSqlNode>
       </dataSqlNode>
	]]>  
  </sql>
