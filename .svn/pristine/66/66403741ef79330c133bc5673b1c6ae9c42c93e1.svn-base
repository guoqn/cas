.step {
    width: 1000px;
    margin: 0 auto;
    padding-top: 10px
}

.step {
    *zoom: 1
}

.step:before, .step:after {
    content: "";
    display: table
}

.step:after {
    clear: both
}

ul.order li {
    width: 33%;
    float: left;
    list-style-type: none;
}

.step ul li {
    width: 25%;
    float: left
}

.step ul li .blueBg, .step ul li .grayBg {
    height: 36px;
    line-height: 36px;
    font-size: 16px;
    color: #fff
}

.step ul li .blueBg {
    background: #3b94dd
}

.step ul li .grayBg {
    background: #c6c6c6
}

.step ul li.stepStar {
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    -webkit-border-top-left-radius: 4px;
    -webkit-border-bottom-left-radius: 4px;
    -moz-border-radius-topleft: 4px;
    -moz-border-radius-bottomleft: 4px
}

.step ul li.stepEnd {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    -webkit-border-top-right-radius: 4px;
    -webkit-border-bottom-right-radius: 4px;
    -moz-border-radius-topright: 4px;
    -moz-border-radius-bottomright: 4px
}

.step ul li.s {
    background-color: #71bafc
}

.step ul li span {
    float: left
}

.step ul li span.num {
    font-size: 20px;
    display: inline-block;
    height: 36px;
    margin-left: 15px
}

.step ul li i.arrowBlueMid {
    background-position: -20px -36px;
    float: right
}

.step ul li i.arrowBlueRight {
    background-position: 0 -36px;
    float: right
}

.step ul li i.arrowBlueEnd {
    background-position: -20px -72px;
    float: left
}

.step ul li i.arrowGrayEnd {
    background-position: -20px 0;
    float: left
}

.step ul li i.arrowGrayRight {
    background-position: 0px 0;
    float: right
}

.step ul li i.arrowGrayRight2 {
    background-position: 0px -72px;
    float: right
}

.step ul li .time {
    font-size: 14px;
    height: 24px;
    line-height: 24px;
    color: #999;
    padding: 8px 15px
}

