/*Top导航条组件*/
.navbar-top {
  background-color: #3b94dd;
  color: white;
  line-height: 66px;
  height: 66px !important;
  width: 100%;
  margin-bottom: 0px;
  font-size: 14px;
  left: 0;
  top: 0;
  _position: absolute;
  border: none;
  border-radius: 0px;
  z-index: 999;
  -webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.3);
  box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.2);
}
.navbar-top .navbar-header {
  height: 100%;
}
.navbar-top .navbar-header .navbar-brand {
  margin-top: 14px;
  padding: 0;
}
.navbar-top .navbar-header .navbar-brand .navbar-ico {
  background-repeat: no-repeat;
  background-postion: center center;
  margin-left: 7px;
}
.navbar-top .navbar-collapse {
  height: 66px !important;
}
.navbar-top .navbar-collapse ul.navbar-nav {
  height: 66px;
}
.navbar-top .navbar-collapse ul.navbar-nav li.active a {
  background-color: #3b94dd;
}
.navbar-top .navbar-collapse ul.navbar-nav li.dropdown {
  min-width: 122px;
}
.navbar-top .navbar-collapse ul.navbar-nav li.open a {
  background-color: #3078b3;
  color: white;
  font-weight: bold;
}
.navbar-top .navbar-collapse ul.navbar-nav li.open a:hover {
  background-color: #3078b3;
  color: white;
}
.navbar-top .navbar-collapse ul.navbar-nav li.open a:focus {
  background-color: #3078b3;
  color: white;
}
.navbar-top .navbar-collapse ul.navbar-nav li.open ul.dropdown-menu li a {
  height: 34px;
  width: 100%;
  font-weight: normal;
}
.navbar-top .navbar-collapse ul.navbar-nav li {
  min-width: 100px;
  height: 66px;
  background-color: #3b94dd;
  text-align: center;
}
.navbar-top .navbar-collapse ul.navbar-nav li a {
  height: 66px;
  line-height: 33px;
  width: 100%;
  color: white;
  background-color: #3b94dd;
}
.navbar-top .navbar-collapse ul.navbar-nav li:hover {
  background-color: #3078b3;
}
.navbar-top .navbar-collapse ul.navbar-nav li:hover a {
  background-color: #3078b3;
  color: #FFF;
  font-weight: bold;
}
.navbar-top .navbar-collapse ul.navbar-nav li:hover ul.dropdown-menu li a {
  font-weight: normal;
}
.navbar-top .navbar-collapse ul.navbar-nav li:hover ul.dropdown-menu li a:hover {
  background-color: #3078b3;
  color: #f1ff15;
}
.navbar-top .navbar-collapse ul.navbar-nav li a.navbar-link {
  color: #f1ff15;
  padding-right: 10px;
  font-size: 14px;
  display: inline;
}
.navbar-top .navbar-collapse ul.navbar-nav li a.navbar-link:hover {
  color: #d7e512;
  font-weight: normal;
  background-color: #3078b3;
}
.navbar-top .navbar-collapse ul.dropdown-menu {
  padding-top: 0px;
  min-width: 116px;
  border-top: none;
  background-color: #3078b3;
}
.navbar-top .navbar-collapse ul.dropdown-menu:focus {
  background-color: #3078b3;
}
.navbar-top .navbar-collapse ul.dropdown-menu li {
  min-width: 116px;
  height: 34px;
  padding: 0 8px;
  background-color: #3078b3;
}
.navbar-top .navbar-collapse ul.dropdown-menu li a {
  width: 116px;
}
.navbar-top .navbar-collapse ul.dropdown-menu li.divider {
  height: 1px;
  margin: 9px 0;
  overflow: hidden;
  background-color: #3b94dd;
}
.navbar-top .navbar-collapse form.navbar-form {
  height: 66px;
  margin: 0px;
}
