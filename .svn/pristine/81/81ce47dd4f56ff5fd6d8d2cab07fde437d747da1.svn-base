}
.fjNet-table tbody tr th,
.fjNet-table tbody tr td {
  text-align: center;
  background-color: white;
}
.fjNet-table tbody tr:hover th,
.fjNet-table tbody tr:hover td {
  cursor: pointer;
  background-color: #F1F9FF;
}
.fjNet-table tr {
  height: 34px !important;
}
.fjNet-table tr th,
.fjNet-table tr td {
  height: 34px !important;
}
.fjNet-table tr th:focus,
.fjNet-table tr td:focus {
  outline-color: #f1f1f1;
}
.fjNet-table tr th:focus *,
.fjNet-table tr td:focus * {
  outline-color: #f1f1f1;
}
.fjNet-table tr:focus {
