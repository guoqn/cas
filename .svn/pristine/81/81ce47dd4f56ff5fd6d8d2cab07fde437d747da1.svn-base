body {
  font: 13px/150% Microsoft yahei, Helvetica, Tahoma, Arial, sim sun;
  color: #333333;
  outline: 0 !important;
  margin: 0 auto;
  background-color: #e9edf1;
  height: 100%;
  width: 100%;
}
body a,
body button {
  outline: 0 !important;
}
body a:focus,
body button:focus {
  outline: 0 !important;
}
.footer-default {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 60px;
  z-index: 5;
  color: #aaaaaa;
  font-size: 12px;
}
.well-default {
  padding: 0;
  border: none;
  margin-bottom: 8px;
  background-color: #FFFFFF;
}
.well-padding {
  padding: 10px 19px 10px 19px;
}
.inline {
  vertical-align: middle;
  display: inline-block;
}
.inline * {
  margin-bottom: 0;
  vertical-align: middle;
}
i.mark {
  float: left;
  display: inline-block;
  width: 3px;
  height: 16px;
  background-color: #ff6500;
  margin: 2px 8px 0 0;
  padding: 0px;
}
.tr-onClick {
  background-color: #F1F9FF;
}
input[type="text"].text {
  border: 1px solid #bcd3c7;
  font-size: 14px;
  line-height: 30px;
  height: 30px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  background-color: #ffffff;
  color: #333333;
    padding-left: 3px;
}
table input {
  background-color: transparent;
}
select.form-control {
  height: 30px;
  font-size: 13px;
}
*.float-right {
  float: right;
}
*.float-left {
  float: left;
}
*.width90 {
  width: 90%;
}
*.width100 {
  width: 100%;
}
.border-radius-default {
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
body.main {
  margin: 0;
  font-size: 14px;
  min-width: 1024px;
  height: 99%;
  font-family: Microsoft YaHei, Verdana;
  line-height: 1.5;
  background-color: #e9edf1;
}
div.div-content {
  margin: 12px 12px 1px 12px;
  padding: 8px 15px 1px 15px;
  background-color: #ffffff;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
}
div.div-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 55px;
  line-height: 55px;
  text-align: right;
  padding-right: 12px;
  background-color: #fdfdfe;
  -webkit-box-shadow: 0px -4px 4px rgba(0, 0, 0, 0.175);
  -moz-box-shadow: 0px -4px 4px rgba(0, 0, 0, 0.175);
  box-shadow: 0px -4px 4px rgba(0, 0, 0, 0.175);
}
div.div-toolbar {
