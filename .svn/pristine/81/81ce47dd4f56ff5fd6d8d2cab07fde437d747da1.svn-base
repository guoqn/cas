/*jquery文件上传 开始*/
.fileinput-button {
  position: relative;
  overflow: hidden;
  display: block;
}

.fileinput-button.inner-blue {
  height: 30px;
  line-height: 30px;
  background-color: #ffffff;
  color: #0090eb;
  padding: 0 20px;
  font-size: 14px;
  border: 1px solid #0090eb;
}

.fileinput-button input {
  position: absolute;
  top: 0;
  right: 0;
  margin: 0;
  opacity: 0;
  -ms-filter: 'alpha(opacity=0)';
  font-size: 200px !important;
  direction: ltr;
  cursor: pointer;
}

/* Fixes for IE < 8 */
@media screen\9 {
  .fileinput-button input {
    filter: alpha(opacity = 0);
    font-size: 100%;
    height: 100%;
  }
}
/*jquery文件上传 结束*/