}
.fjNet-table thead {
  background-color: #f2f2f2;
}
.fjNet-table thead small {
  color: #999999;
}
/*自定义菜单栏*/
.nav-tab-default {
  font-size: 15px;
  overflow: hidden;
  zoom: 1;
  padding-left: 1px;
  height: 38px;
  background-color: #f8f8f8;
}
.nav-tab-default .nav-justified {
  width: 100%;
  border-bottom: 0;
}
.nav-tab-default .nav-justified li {
  float: none;
}
.nav-tab-default .nav-justified li a {
  margin-bottom: 5px;
  text-align: center;
}
.nav-tab-default .nav-justified .dropdown .dropdown-menu {
  top: auto;
  left: auto;
}
.nav-tab-default li {
  height: 38px;
  background: #F9FAFC;
  margin-left: -1px;
  border: 1px solid #ccc;
  float: left;
  font-weight: bold;
  cursor: pointer;
  padding: 0 15px;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}
.nav-tab-default li a:link {
  padding: 6px;
  color: #666;
  background-color: #F9FAFC;
  border: none;
  outline: none;
}
.nav-tab-default li a:hover {
  padding: 6px;
  color: #666;
  background-color: #F9FAFC;
  border: none;
  outline: none;
}
.nav-tab-default li a:focus {
  padding: 6px;
  color: #666;
  background-color: #F9FAFC;
  border: none;
  outline: none;
}
.nav-tab-default li.active {
  font-weight: bold;
  background: #fff;
  border: 1px solid #ccc;
  border-bottom: none;
  border-top: 4px solid #62b5f8;
}
.nav-tab-default li.active a[data-toggle="tab"] {
  color: #10a6ee;
  border: none;
  background-color: white;
}
.nav-tab-default li.active:hover {
  font-weight: bold;
  background: #fff;
  border: 1px solid #ccc;
  border-bottom: none;
  border-top: 4px solid #62b5f8;
}
.nav-tab-default li.active:focus {
  font-weight: bold;
  background: #fff;
  border: 1px solid #ccc;
  border-bottom: none;
  border-top: 4px solid #62b5f8;
}
.nav-tab-default li:first-child {
  border-left: none;
}
.nav-tab-default li:first-child:hover {
  border-left: none;
}
.nav-tab-content-default {
  border-top: 1px solid #c8d1d5;
  margin-top: -1px;
  padding: 19px 10px;
}
.nav-tab-content-default .tab-pane .panel.panel-default {
  border: none;
}
